@model IEnumerable<WebApplication1.Entities.Product>

<h2>Product List</h2>
<a asp-action="Create">Add New Product</a>
<table border="1">
	<tr>
		<th>Name</th>
		<th>Quantity</th>
		<th>Price</th>
		<th>Actions</th>
	</tr>
	@foreach (var item in Model)
	{
		<tr>

			<td>@item.Name</td>
			<td>@item.Quantity</td>
			<td>@item.Price</td>
			<td>
				<a asp-action="Edit" asp-route-id="@item.Id">Edit</a>
				<a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
			</td>
		</tr>
	}
</table>